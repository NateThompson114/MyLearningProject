@page "/"
@using DependencyInjection.Data
@inject Demo Demo
@inject ProcessDemo ProcessDemo

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.
<p>
    Startup time @Demo.StartupTime.ToLocalTime().ToString("hh:mm:ss fffff")
    <br />
    <i>Days in Month: @ProcessDemo.GetDaysInMonth()</i>
</p>


<SurveyPrompt Title="How is Blazor working for you?" />
